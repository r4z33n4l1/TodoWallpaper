{
  "expo": {
    "name": "TodoWallpaper",
    "slug": "TodoWallpaper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ToDo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/ToDo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.r4z33n4l1.todowallpaper",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "TodoWallpaper needs access to your photo library to save your todo list as a wallpaper image. This allows you to set your tasks as your lock screen or home screen background.",
        "NSPhotoLibraryAddUsageDescription": "TodoWallpaper saves your todo list as a wallpaper image to your photo library. This image can then be used as your lock screen or home screen background to keep your tasks visible."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ToDo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.r4z33n4l1.todowallpaper"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ToDo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "449ac577-8be2-413b-a8cf-8790d655b6af"
      }
    }
  }
}
